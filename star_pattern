/*Code to print star patters
for example: n=6
*	
*	*	
*	*	*	
*	*	*	*	
*	*	*	*	*	
*	*	*	*	*	*
*/



#include<iostream>
using namespace std;

int main(){
    int n;
    cout<<"enter n";
    cin>>n;
    
    for(int i=1;i<=n;i++){
    for(int j=1;j<=i;j++){
        cout<<"*"<<"\t";
    }
    cout<<"\n";
    }
}


